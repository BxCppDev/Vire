			<Value>Panazol Jean-luc</Value>
		</Attribut>
		<Attribut>
			<Name>DeviceModelDescription</Name>
			<Value></Value>
		</Attribut>
		<Attribut>
			<Name>InstanceName</Name>
			<Value>SNEMO/TRACKER/TRACKER_TRIGGER/@Name@</Value>
		</Attribut>
		<Attribut>
